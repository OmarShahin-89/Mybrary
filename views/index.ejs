<div class="container">
    <h2>Recently Added</h2>
    <div class="row">
        <% books.forEach(book=>{ %> 
            <div class="col-sm-6 col-md-4 col-lg-3">
                <a href="/books/<%= book.id %>">
                    <img  class="img-thumbnail" src="<%= book.coverImagePath %>" alt="" height="150" width="100">
                </a>
            </div>
        <%})%>
    </div>
</div>